{
    "name" : "stackedy",
    "description" : "Roll your own stack traces and control program execution through AST manipulation",
    "version" : "0.1.0",
    "repository" : {
        "type" : "git",
        "url" : "git://github.com/substack/node-stackedy.git"
    },
    "main" : "./index.js",
    "keywords" : [
        "stack",
        "trace",
        "ast",
        "error",
        "exception",
